{
  "name": "@yashdalfthegray/dsts",
  "version": "1.0.0",
  "description": "data structures but in Typescript",
  "main": "dist/index.js",
  "scripts": {
    "test": "nyc ava",
    "test:no-coverage": "ava",
    "test:ci": "nyc ava && nyc report --reporter=text-lcov | coveralls",
    "linter": "tslint --fix --project .",
    "open-coverage-report": "npx open ./coverage/index.html",
    "prebuild": "npx rimraf dist",
    "build": "tsc",
    "prepare": "npm run snyk-protect && npm test && npm run build",
    "snyk-protect": "snyk protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YashdalfTheGray/dsts.git"
  },
  "keywords": [
    "data",
    "structures",
    "typescript"
  ],
  "author": "Yash Kulshrestha",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/YashdalfTheGray/dsts/issues"
  },
  "homepage": "https://github.com/YashdalfTheGray/dsts#readme",
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.spec.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "ava": "^3.11.1",
    "coveralls": "^3.0.9",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "nyc": "^14.1.1",
    "prettier": "^1.19.1",
    "snyk": "^1.370.1",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.7.3"
  },
  "dependencies": {
    "@types/lodash": "^4.14.149",
    "lodash": "^4.17.16"
  },
  "snyk": true
}
