{
  "name": "dsts",
  "version": "1.0.0",
  "description": "data structures but in Typescript",
  "main": "dist/index.js",
  "scripts": {
    "test": "ava",
    "prebuild": "npx rimraf dist",
    "build": "tsc",
    "prepare": "npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YashdalfTheGray/dsts.git"
  },
  "keywords": [
    "data",
    "structures",
    "typescript"
  ],
  "author": "Yash Kulshrestha",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/YashdalfTheGray/dsts/issues"
  },
  "homepage": "https://github.com/YashdalfTheGray/dsts#readme",
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "ava": "^2.2.0",
    "husky": "^3.0.2",
    "lint-staged": "^9.2.1",
    "prettier": "^1.18.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@types/lodash": "^4.14.136",
    "lodash": "^4.17.15"
  }
}
